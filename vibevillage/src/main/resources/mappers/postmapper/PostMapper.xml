        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.vibevillage.talentgroupboard.postmapper.PostMapper">

    <select id="findAll" resultType="kr.co.vibevillage.talentgroupboard.model.TalentGroupBoardDTO">
